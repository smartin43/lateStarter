<div class="container">
  <div class="jumbotron">
    <h1 class="text-center">Login</h1>
    <%= form_tag('/try_login', method:"get", class:"form-horizontal") do %>
      <%= label_tag :username, nil, class:"control-label" %><br/>
      <%= text_field_tag :username, nil, class:"form-control" %><br/>
      <%= label_tag :password, nil, class:"control-label"%><br/>
      <%= password_field_tag :password, nil, class:"form-control" %><br/>
      <%= submit_tag "login", class:"btn btn-primary form-control" %>
    <% end %>
  </div>
</div>
